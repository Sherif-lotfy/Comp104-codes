#include <iostream>
using namespace std;
int main(){
int x,a,b,c,d,e,f,g,h,i,j,k;
cout<<"enter the 6-digit number"<<endl;
cin>>x;
a=x/100000;
b=x-(a*100000);
c=b/10000;
d=b-(c*10000);
e=d/1000;
f=d-(e*1000);
g=f/100;
h=f-(g*100);
i=h/10;
j=h-(i*10);
cout<<a+c+e+g+i+j<<endl;
system("pause");
return 0;

}
